{
    "name": "inoplate/config",
    "description": "Inoplate configuration repository",
    "keywords": ["database", "config", "laravel", "php"],
    "type": "composer",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Roseffendi",
            "email": "oii@roseffendi.com"
        }   
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Inoplate\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Inoplate\\Config\\Tests\\Laravel\\App\\": "tests/laravel/app"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/cache": "^5.3"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^2.3",
        "phpspec/phpspec": "^3.0",
        "laravel/laravel": "dev-develop",
        "benconstable/phpspec-laravel": "~3.0@dev",
        "phpunit/phpunit": "^5.5"
    }
}
